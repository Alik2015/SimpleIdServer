{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdServerBaseUrl": "https://localhost:5001",
  "ScimBaseUrl": "https://localhost:5003",
  "DefaultSecurityOptions": {
    "Issuer": "https://localhost:5001/master",
    "ClientId": "SIDS-manager",
    "ClientSecret": "password",
    "Scope": "openid profile",
    "IgnoreCertificateError": false
  },
  "StorageConfiguration": {
    "ConnectionString": "Data Source=.;Initial Catalog=IdServer;Integrated Security=True;TrustServerCertificate=True",
    "Type": "SQLSERVER"
  },
  "DetailedErrors": true
}
